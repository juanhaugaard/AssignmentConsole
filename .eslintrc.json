{
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 0,
        "space-before-function-paren": 0,
        "no-useless-constructor": 0,
        "no-unused-vars": 1
    }
}